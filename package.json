  "bugs": {
    "url": "https://github.com/FreskyZ/home-fresky/issues"
  },
  "homepage": "https://github.com/FreskyZ/home-fresky#readme",
  "dependencies": {
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "source-map": "^0.7.3",
    "stacktrace-js": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/filesize": "^4.0.1",
    "@types/mysql": "^2.15.5",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "chalk": "^2.4.2",
    "csstype": "^3.0.4",
