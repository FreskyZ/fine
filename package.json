{
  "name": "home-fresky",
  "version": "0.1.0",
  "description": "a personal website for fun",
  "scripts": {
    "build": "node scripts/build.js",
    "build-client": "./build-client",
    "build-client-logs": "tsc --project tsconfig/client.logs.json",
    "build-client-index": "tsc --project tsconfig/client.index.json && lessc src/client/index.less asset/static/index.css",
    "build-server": "tsc --project tsconfig/server.json",
    "start-server": "tsc --project tsconfig/server.json && nodejs build/server/index.js",
    "dev-main": "tsc --project tsconfig/server.json && node build/server/dev-main.js",
    "stop-server": "pkill --signal SIGINT nodejs || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FreskyZ/home-fresky.git"
  },
  "author": "FreskyZ",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FreskyZ/home-fresky/issues"
  },
  "homepage": "https://github.com/FreskyZ/home-fresky#readme",
  "dependencies": {
    "express": "^4.15.4",
    "moment": "^2.24.0",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.12.21",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "chalk": "^2.4.2",
    "csstype": "^2.6.2",
    "minimist": "^1.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "script-loader": "^0.7.2",
    "typescript": "^3.3.1",
    "webpack": "^4.29.5"
  }
}
