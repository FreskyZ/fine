{
    "apis": [
        {
            // default namespace is default, api in default namespace is imported from 'server' ('server/index.ts')
            "name": "getValue1s",
            "method": "GET",
            "path": "/value1s",
            "return": "Value1[]"
        },
        {
            "name": "getValue1",
            "method": "GET",
            "path": "/some-value1/{valueId:number}", // path should not have whitespace, include around the ':'
            "return": "Value1"
        },
        // /^(?<namespace>\w+\.)?(?<name>\w+): (?<method>(GET|POST|PUT|PATCH|DELETE)) (?<path>[\w\/\{\-:]+) \(((?<body_name>\w+): (?<body_type>\w+))?\) => (?<return_type>\w+)/
        // path parameter type can be number, string, boolean, date and datetime
        // number is simply \d+, string is .+, boolean is true/false, date is YYYYMMMDD, datetime is YYYYMMDDHHmmdd
        "namespace.updateValue1: POST /value1/{valueId:number} (value: Value1) => Value1",
        {
            "name": "updateValue1",
            "method": "POST",
            "path": "/value1/{valueId:number}",
            "body": "Value1",
            "return": "Value1"
        },
        {
            "name": "addValue1",
            "method": "PUT",
            "path": "/value1s",
            "body": "Value1",
            "return": "Value1"
        }
    ]
}